{
  "name": "makestack-installer",
  "version": "0.0.1",
  "license": "SEE LICENSE IN LICENSE.md",
  "main": "./main/index.js",
  "description": "",
  "scripts": {
    "build:macos": "webpack && electron-packager . --platform darwin --asar --overwrite --out dist --arch x64",
    "build:linux": "webpack && electron-packager . --platform linux  --asar --overwrite --out dist --arch x64",
    "dev": "concurrently --kill-others 'yarn run main' 'yarn run renderer'",
    "main": "NODE_ENV=development ../tools/run-gui-app npx electron .",
    "renderer": "webpack-dev-server --hot --inline --port 9080"
  },
  "dependencies": {
    "makestack": "file:../cli",
    "node-fetch": "^1.7.3",
    "vue": "^2.4.4",
    "vue-router": "^2.7.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "cfonts": "^1.1.3",
    "chalk": "^2.1.0",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.1.1",
    "css-loader": "^0.28.7",
    "electron": "^1.7.8",
    "electron-packager": "^9.1.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-hot-middleware": "^2.19.1"
  }
}
