  },
  "devDependencies": {
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^4.0.1",
    "mock-fs": "^4.4.2",
    "nan": "^2.7.0",
    "power-assert": "^1.4.4",
    "sinon": "^4.1.2"
  }
}
